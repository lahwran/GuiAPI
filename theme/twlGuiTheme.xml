        </theme>
        <theme name="rightbutton" ref="scrollbutton-notch">
            <param name="minWidth"><int>6</int></param>
        </theme>
        <theme name="upbutton" ref="scrollbutton-notch">
            <param name="minHeight"><int>6</int></param>
        </theme>
